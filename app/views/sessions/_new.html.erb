<div id='login-form'>
<h1>Log In</h1>

  <%=form_tag log_in_path, remote: true do%>
    <%=label_tag :email %>
    <%=text_field_tag :email %><br>
    <%=label_tag :password %>
    <%=password_field_tag :password %><br>
    <%=submit_tag "Log in" %>
  <%end%>
  <%= link_to 'Sign Up', new_user_path %>
</div>
