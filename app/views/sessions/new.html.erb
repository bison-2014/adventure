<h1>Log In</h1>
<% flash.each do |name, msg| %>
  <%= content_tag :div, msg, :id => "flash_#{name}" %>
<% end %>
  <%=form_tag log_in_path do%>
    <%=label_tag :email %>
    <%=text_field_tag :email %><br>
    <%=label_tag :password %>
    <%=password_field_tag :password %><br>
    <%=submit_tag "Log in" %>
  <%end%>
  <%= link_to 'Sign Up', new_user_path %>